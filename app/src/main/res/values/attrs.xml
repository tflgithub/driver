<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RecyclerViewPager">
        <attr name="rvp_triggerOffset" format="float" />
        <attr name="rvp_flingFactor" format="float" />
        <attr name="rvp_singlePageFling" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SlideBottomPanel">
        <attr name="sbp_background_layout" format="reference"/>
        <attr name="sbp_title_height_no_display" format="dimension"/>
        <attr name="sbp_panel_height" format="dimension"/>
        <attr name="sbp_boundary" format="boolean"/>
        <attr name="sbp_move_distance_trigger" format="dimension"/>
        <attr name="sbp_animation_duration" format="integer"/>
        <attr name="sbp_fade" format="boolean"/>
        <attr name="sbp_hide_panel_title" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ViewfinderView">
        <attr name="corner_color" format="color"/>
        <attr name="laser_color" format="color"/>
        <attr name="frame_color" format="color" />
        <attr name="mask_color" format="color" />
        <attr name="result_point_color" format="color"/>
        <attr name="result_color" format="color" />
        <attr name="label_text_color" format="color"/>
        <attr name="label_text" format="string"/>
        <attr name="label_text_size" format="float"/>
    </declare-styleable>

    <declare-styleable name="CircleTextView">
        <!-- Default Paint Color (color). -->
        <attr name="paintNormalColor" format="reference|color"/>
    </declare-styleable>
</resources>
